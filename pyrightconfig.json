{
  "include": [
    "src",
    "tests",
    "scripts",
    "database"
  ],
  "exclude": [
    "venv",
    ".pytest_cache",
    "htmlcov",
    "ml_models"
  ],
  "pythonVersion": "3.12",
  "typeCheckingMode": "standard",
  "reportMissingImports": true,
  "reportMissingTypeStubs": false,
  "pythonPlatform": "Windows",
  "executionEnvironments": [
    {
      "root": ".",
      "extraPaths": ["./src", "./database", "./scripts"]
    }
  ]
}